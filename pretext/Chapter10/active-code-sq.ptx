#include <vector>
using namespace std;

int evenCount(const vector<int> &vec) {
    for (int i = 0; i < vec.size(); i++) {
        if (i % 2 == 0) {
            count++;
        }
    }
    return count;
}

int main() {
    vector<int> vec{1,2,3,4};
    cout << evenCount(vec) << endl;
}
