    int count = 0;
    for (size_t row = 0; row &lt; board.size(); ++row) {
        for (size_t col = 0; col &lt; board[row].size(); ++col) {
        board[row][col].value = vec[count];
        ++count;
        }
    }

    // Create free space
    board[2][2].value = 0;
    board[2][2].is_filled = true;
}
