  int suit, rank;
  Card ();
  Card (int s, int r);
};

Card::Card () {
  suit = 0;  rank = 0;
}

Card::Card (int s, int r) {
  suit = s;  rank = r;
}

int main() {
  Card card (2, 8);
}
</input>
      </program>
    </statement>
